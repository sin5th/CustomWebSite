{
  "background": {
    "scripts": [
      "resource/jquery.min.js",
      "scripts/background.js"
    ]
  },
  "browser_action": {
    "default_icon": "resource/icon.png",
    "default_title": "Custom Web Site"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval' https://ssl.google-analytics.com; object-src 'self'",
  "description": "Custom request and response headers, init scripts, style sheets",
  "incognito": "spanning",
  "manifest_version": 2,
  "name": "CustomWebSite",
  "permissions": [
    "unlimitedStorage",
    "storage",
    "webRequest",
    "webRequestBlocking",
    "<all_urls>"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "0.1.0"
}
